{
  "name": "auto-click",
  "version": "1.0",
  "description": "An addin for Tieba.",
  "icons": {
    "16": "./hehe.jpg",
    "128": "./hehe.jpg"
  },
  "permissions": ["<all_urls>", "webRequest", "tabs"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup.js"],
      "run_at": "document_start"
    }
  ],
  "manifest_version": 2
}
